	<component name="behaviorSelector" class="saaya.murakami.dbflute.allcommon.ImplementedBehaviorSelector"/>
	<component name="commonColumnAutoSetupper" class="saaya.murakami.dbflute.allcommon.ImplementedCommonColumnAutoSetupper"/>

	<!-- The components of Behavior. -->
	<component name="accountBhv" class="saaya.murakami.dbflute.exbhv.AccountBhv"/>
